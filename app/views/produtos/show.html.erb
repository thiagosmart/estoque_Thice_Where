<div class="page-header">
  <%= link_to produtos_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_produto_path(@produto), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show produto</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @produto.nome %></dd>

  <dt>Cor:</dt>
  <dd><%= @produto.cor %></dd>

  <dt>Tamanho:</dt>
  <dd><%= @produto.tamanho %></dd>

  <dt>Posicao estocada:</dt>
  <dd><%= @produto.posicao_estocada %></dd>

  <dt>Preco compra:</dt>
  <dd><%= @produto.preco_compra %></dd>

  <dt>Preco venda:</dt>
  <dd><%= @produto.preco_venda %></dd>

  <dt>Descricao:</dt>
  <dd><%= @produto.descricao %></dd>

  <dt>Fornecerdor:</dt>
  <dd><%= @produto.fornecerdor %></dd>

</dl>
